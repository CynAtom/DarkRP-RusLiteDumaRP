"VertexLitGeneric"
{
	"$basetexture" "models\dawson\obese_male_deluxe\obese_male_deluxe_cloth"
	"$model" 1

	"$blendtintbybasealpha" "1" 
	"$blendtintcoloroverbase" "0"
Proxies{
	PlayerColor{
		resultVar	$color2
		default 0.75 0.75 0.75
		}
	Multiply // doing this to make brighter & more saturated
		{
			"srcVar1" "$selfillumtint"
			"srcVar2" "$rimlightboost"
			"resultVar" "$selfillumtint"
		}
			Clamp // clamp the chosen color between 1% and 125% (usually)
			{
				min 0.175
				max 1.25
				srcVar1 "$color2"
				resultVar "$color2"
			}

		}
}
